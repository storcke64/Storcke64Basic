      }
      { Menu2 Menu mnuSize
        Name = "Menu2"
        Text = ("Small")
        Radio = True
        Tag = "1"
      }
      { Menu3 Menu mnuSize
        Name = "Menu3"
        Text = ("Medium")
        Radio = True
        Tag = "2"
      }
      { Menu4 Menu mnuSize
